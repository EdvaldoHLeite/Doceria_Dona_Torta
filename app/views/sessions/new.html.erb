<% provide(:title, "Login") %>
<h1> Log in </h1>

<%= form_tag sessions_path do |form| %>
  <%= label_tag :email %>
  <%= text_field_tag :email, nil, placeholder: 'email'%>

  <%= label_tag :password %>
  <%= password_field_tag :password, nil, placeholder: "password" %>

  <%= submit_tag "Login" %>

<% end %>